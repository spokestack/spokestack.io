{
  "name": "spokestack-website",
  "private": true,
  "description": "Mobile voice development platform that enables customized voice navigation for mobile and browser applications",
  "version": "0.1.0",
  "author": "Timmy Willison <4timmywil@gmail.com>",
  "scripts": {
    "build": "yarn clean && gatsby build",
    "clean": "gatsby clean",
    "commit": "git-cz",
    "deploy": "gatsby build && gh-pages -d public -b master",
    "format": "prettier --write src/**/*.{js,jsx}",
    "lint": "concurrently --raw \"yarn type-check\" \"yarn eslint\" \"yarn prettier\"",
    "lint:fix": "concurrently --raw \"yarn type-check\" \"lint-staged\"",
    "post": "node tasks/post.js",
    "prettier": "prettier --check \"**/*.tsx\" \"**/*.ts\" \"**/*.js\"",
    "release": "semantic-release",
    "schema": "graphql-codegen --config codegen.yml",
    "serve": "gatsby serve",
    "start": "yarn clean && gatsby develop",
    "eslint": "eslint .",
    "type-check": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "gatsby": "^2.13.41",
    "gatsby-image": "^2.2.7",
    "gatsby-plugin-emotion": "^4.1.2",
    "gatsby-plugin-feed": "^2.3.5",
    "gatsby-plugin-google-analytics": "^2.1.4",
    "gatsby-plugin-manifest": "^2.2.4",
    "gatsby-plugin-offline": "^3.0.17",
    "gatsby-plugin-optimize-svgs": "^1.0.3",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-sharp": "^2.2.9",
    "gatsby-plugin-svg-sprite": "^2.0.1",
    "gatsby-plugin-typescript": "^2.1.2",
    "gatsby-plugin-typography": "^2.3.2",
    "gatsby-remark-copy-linked-files": "^2.1.3",
    "gatsby-remark-images": "^3.1.7",
    "gatsby-remark-prismjs": "^3.3.3",
    "gatsby-remark-responsive-iframe": "^2.2.4",
    "gatsby-remark-smartypants": "^2.1.2",
    "gatsby-source-filesystem": "^2.1.6",
    "gatsby-transformer-remark": "^2.6.9",
    "gatsby-transformer-sharp": "^2.2.4",
    "prismjs": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typeface-crimson-text": "^0.0.72",
    "typeface-rosario": "^0.0.72",
    "typescript": "^3.5.3",
    "typography": "^0.16.19",
    "typography-breakpoint-constants": "^0.16.19"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@commitlint/prompt": "^8.1.0",
    "@graphql-codegen/cli": "^1.4.0",
    "@graphql-codegen/typescript": "^1.4.0",
    "@semantic-release/exec": "^3.3.5",
    "@semantic-release/git": "^7.0.16",
    "@types/react-helmet": "^5.0.8",
    "@types/typography": "^0.16.2",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "commitizen": "^4.0.3",
    "concurrently": "^5.0.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "gh-pages": "^2.0.1",
    "graphql": "^14.4.2",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.1",
    "lodash.template": "^4.5.0",
    "mkdirp": "^0.5.1",
    "prettier": "^1.18.2",
    "semantic-release": "^15.13.18"
  },
  "resolutions": {
    "lodash": ">=4.17.15"
  },
  "bugs": {
    "url": "https://github.com/spokestack/spokestack-website/issues"
  },
  "homepage": "https://github.com/spokestack/spokestack-website#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/spokestack/spokestack-website.git"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "arrowParens": "always"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@commitlint/prompt"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "env FORCE_COLOR=1 yarn lint:fix"
    }
  },
  "release": {
    "branch": "develop",
    "ci": false,
    "npmPublish": false,
    "pre-commit": "yarn lint:fix",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "yarn deploy"
        }
      ],
      "@semantic-release/github"
    ],
    "tagFormat": "${version}"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix .",
      "git add"
    ]
  }
}
